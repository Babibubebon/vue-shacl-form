{
  "name": "vue-shacl-form",
  "version": "0.3.1",
  "description": "A Vue.js component for HTML5 form generation based on SHACL",
  "author": "Shohei Toyota",
  "scripts": {
    "serve": "vue-cli-service serve --mode bundle",
    "build": "vue-cli-service build --modern --target lib --formats umd,umd-min --name VueShaclForm src/ShaclForm.vue && rm ./dist/demo.html",
    "lint": "vue-cli-service lint",
    "build:demo": "vue-cli-service build --dest demo --mode bundle src/main.js"
  },
  "main": "dist/VueShaclForm.umd.js",
  "dependencies": {
    "core-js": "^3.6.5",
    "merge": "^1.2.1",
    "rdflib": "^1.3.1",
    "shacl": "github:Babibubebon/shacl-js#develop"
  },
  "devDependencies": {
    "@babel/runtime-corejs2": "^7.11.2",
    "@vue/cli-plugin-babel": "^4.5.6",
    "@vue/cli-plugin-eslint": "^4.5.6",
    "@vue/cli-service": "^4.5.6",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.5.2",
    "eslint": "^7.10.0",
    "eslint-plugin-vue": "^7.0.0",
    "jquery": "^3.5.1",
    "solid-auth-cli": "^1.0.15",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/require-v-for-key": "off",
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Babibubebon/vue-shacl-form.git"
  }
}
